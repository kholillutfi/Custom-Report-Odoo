<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
            
        <record id="view_mutation_report_wizard" model="ir.ui.view">
            <field name="name">mutation_report.wizard.form</field>
            <field name="model">mutation.report.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group col="4">
                        <field name="start_date" required="1"/>  
                        <field name="categ_id"/>
                        <field name="end_date" required="1"/>
                        <field name="company_id" readonly="1"/>
                    </group> 
                    <footer>
                        <button string="Print" name="print_report" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>       
    
    </data>
    
</odoo>
